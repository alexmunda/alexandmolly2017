{
  "name": "alexandmolly2017",
  "version": "1.0.0",
  "description": "Alex and Molly wedding website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && node dist_server/server.js",
    "start:dev": "npm run clean && npm run build:dev & nodemon src/server.js --exec babel-node ",
    "build": "webpack --config webpack.dev.config.babel.js --env.prod && mkdir dist_server && babel src --out-dir dist_server",
    "build:dev": "webpack --config webpack.dev.config.babel.js --env.dev --watch",
    "clean": "del-cli dist dist_server",
    "test": "test",
    "lint": "eslint src/**"
  },
