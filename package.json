    "url": "https://github.com/alexmunda/alexandmolly2017/issues"
  },
  "homepage": "https://github.com/alexmunda/alexandmolly2017#readme",
  "dependencies": {
    "app-root-path": "^2.0.0",
    "babel-cli": "^6.14.0",
    "classnames": "^2.2.5",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.97.7",
    "react": "^15.3.1",
    "react-count-down": "1.0.3",
