{
  "name": "alexandmolly2017",
  "version": "1.0.0",
  "description": "Alex and Molly wedding website",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run build && NODE_ENV=production node dist_server/server.js",
    "start:dev": "npm run clean && npm run build:dev & nodemon src/server.js --exec babel-node",
    "build": "webpack --config webpack.dev.config.babel.js --env.prod && mkdir dist_server && babel src --out-dir dist_server",
    "build:dev": "webpack --config webpack.dev.config.babel.js --env.dev --watch",
    "clean": "del-cli dist dist_server",
    "db:create": "createdb alexandmolly2017",
    "test": "test",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmunda/alexandmolly2017.git"
  },
  "author": "Alex Munda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexmunda/alexandmolly2017/issues"
  },
  "homepage": "https://github.com/alexmunda/alexandmolly2017#readme",
  "dependencies": {
    "app-root-path": "2.0.0",
    "babel-cli": "6.14.0",
    "body-parser": "1.15.2",
    "classnames": "2.2.5",
    "express": "4.14.0",
    "jade": "1.11.0",
    "jsonwebtoken": "7.1.9",
    "pg": "6.1.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.14.0",
    "css-loader": "0.24.0",
    "del-cli": "0.2.0",
    "elm-webpack-loader": "^3.0.6",
    "file-loader": "0.9.0",
    "lodash": "4.15.0",
    "nodemon": "1.10.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-validator": "2.2.7"
  }
}
