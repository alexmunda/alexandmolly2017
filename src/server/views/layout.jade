doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible', content='IE=Edge', charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(name="description" content="Alex Munda and Molly DiGiacinto's wedding website. RSVP, view photos, and check out the registry.")
    meta(name='keywords' content='Alex Munda, Molly DiGiacinto, October 28, Wedding')
    meta(name='author' content='Alex Munda')
    block title
      title= title
    link(rel='stylesheet', href='/assets/stylesheets/bootstrap.min.css')
    link(rel='stylesheet', href=WebpackPath('main.css'))
    block styles

  body
    block content

    block scripts
      script.
        window.alexandmolly = window.alexandmolly || {}

      include tracker

      script(src=WebpackPath('main.js'))
